class Bank {
   int property; // 자산
   int moeny;

   Bank(int moeny) {
      this.moeny = moeny;
   }

   void Info() {
      
   }

   

}

class KHbank extends Bank {
   KHbank(int moeny) {
      super(moeny);
   }
   
   void Info() { // 잔고
//       moeny 
   }

   
}

class YHbank extends Bank {
   YHbank(int moeny) {
      super(moeny);
      
   }

   void Info() { // 잔고
      
   }

}

class JHbank extends Bank {
   JHbank(int moeny) {
      super(moeny);
      
   }

   void Info() { // 잔고
      
   }

   

}

class BankUser {
   int moeny; // 현금

   BankUser(int moeny) {
      this.moeny = moeny;
   }

   void withDraw(Bank bank, int draw) { //출금
      this.moeny += draw;
      bank.moeny -= draw;
   }
   

   void deposit(String string, int draw) { //입금
      this.moeny += draw;
      
   }

   void loan(Bank name, int draw) {

   }
}

public class Practice {

   public static void main(String[] args) {
      BankUser user = new BankUser(400);
      System.out.println("내 현재 현금은" + user.moeny + "만원 입니다");
      
      
      
      
      KHbank k = new KHbank(100);
      System.out.println("내 현재 KH은행은" + k.moeny + "만원 입니다");
      user.withDraw(k,100);
      System.out.println("내 현재 KH은행은" + k.moeny + "만원 입니다");   
      System.out.println("내 현재 현금은" + user.moeny + "만원 입니다");
      YHbank y = new YHbank(300);
      
      JHbank j = new JHbank(200);

   }

}