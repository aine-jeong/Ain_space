
interface Loanable{ 
//    int loanable_money = 300;
}

class AinLoan implements Loanable{
    
    int loanable_money = 300;
    
}

class Bank{
    int money;
   
    Bank(int money) {
        this.money = money;
    }
    
    void withDraw(int money) {
        this.money -= money;
    }
    
    void deposit(int money) {
        this.money += money;
    }
}

class KHBank extends Bank{
    KHBank(int money) {
        super(money);
    }
}

class YHBank extends Bank implements Loanable{

    int loanable_money = 300;
    
    YHBank(int money) {
        super(money);
    }
}

class JHBank extends Bank implements Loanable{

    int loanable_money = 300;
    
    JHBank(int money) {
        super(money);
    }
}

class User{
    int Wallet = 5000;
    
    void bankUtil(Bank bank){
        
    }
    
    void loan(Loanable loan) {
        
        Bank bank = (Bank)loan;
        
         
    }
}

public class HW_Diagram {
    public static void main(String[] args) {
        User user = new User();
        KHBank khbank = new KHBank(1000);
        YHBank yhbank = new YHBank(500);
        JHBank jhbank = new JHBank(300);
        AinLoan ainloan = new AinLoan();
        
        user.loan(yhbank);
    }

}
